<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    style="@style/GuidRecyclerItemTwo"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal">

    <ImageView
        android:id="@+id/GuidItemTwoCardIV"
        style="@style/GuidRecyclerItemTwo"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:scaleType="fitXY"
        android:clickable="true"
        android:src="@mipmap/e"
        />

    <TextView
        android:id="@+id/GuidItemTwoCardTitleTV"
        style="@style/GuidRecyclerItemTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="20dp"
        android:paddingBottom="10dp"
        android:text="小桥流水人家"
        android:textSize="15sp"
        android:textColor="@color/colorWhite"
        />

    <ImageView
        android:id="@+id/GuidItemTwoCardIvNEW"
        style="@style/GuidRecyclerItemTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/ig_marker_fresh"
        />

    <com.example.administrator.sh1512_5.MyView.MylittleStar
        android:id="@+id/GuidItemTwoCardMylittleStar"
        style="@style/GuidRecyclerItemTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="20dp">
    </com.example.administrator.sh1512_5.MyView.MylittleStar>
</RelativeLayout>
